{
  "name": "ai-news-aggregator",
  "version": "1.0.0",
  "description": "Daily news aggregator for AI-related tech job news from around the USA",
  "main": "dist/index.js",
  "bin": {
    "ai-news": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "run": "npm run dev run",
    "schedule": "npm run dev start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "news",
    "aggregator",
    "tech-jobs",
    "rss",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "rss-parser": "^3.13.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
